{
  "name": "speechmatics-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "run-p build:jobs-api-ref && dotenv -e .env -e .env.local node_modules/.bin/docusaurus start",
    "build": "run-p build:jobs-api-ref && dotenv -e .env -e .env.local node_modules/.bin/docusaurus build",
    "build:jobs-spec": "tsx scripts/generate-batch-spec.ts",
    "build:jobs-api-ref": "rm -rf docs/api-ref/batch && npm run build:jobs-spec && docusaurus gen-api-docs jobs",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "spellcheck": "npx cspell **/*.md **/*.mdx",
    "sync-redirects": "tsx scripts/redirects/sync-redirects.ts"
  },
  "dependencies": {
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@docusaurus/theme-live-codeblock": "^3.8.1",
    "@docusaurus/theme-mermaid": "^3.8.1",
    "@mdx-js/react": "^3.0.0",
    "@radix-ui/react-compose-refs": "^1.1.2",
    "@radix-ui/themes": "^3.2.1",
    "@signalwire/docusaurus-plugin-llms-txt": "^1.2.1",
    "asyncapi-schema-loader": "^0.0.1",
    "clsx": "^2.0.0",
    "docusaurus-plugin-openapi-docs": "^4.4.0",
    "docusaurus-theme-openapi-docs": "^4.4.0",
    "lucide-react": "^0.512.0",
    "mixpanel-browser": "^2.67.0",
    "openapi-schema-loader": "^1.0.1",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "3.0.1",
    "remark-math": "^6.0.0"
  },
  "devDependencies": {
    "@asyncapi/parser": "^3.4.0",
    "@biomejs/biome": "^1.9.4",
    "@docusaurus/module-type-aliases": "^3.8.1",
    "@docusaurus/tsconfig": "^3.8.1",
    "@docusaurus/types": "^3.8.1",
    "@types/mixpanel-browser": "^2.60.0",
    "cspell": "^8.17.3",
    "dotenv": "^17.2.0",
    "dotenv-cli": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^4.0.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.4",
    "typescript": "~5.6.2",
    "yaml": "^2.8.0"
  },
  "overrides": {
    "postman-code-generators": "1.10.1"
  },
  "browserslist": {
    "production": [">0.5%", "not dead", "not op_mini all"],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
